<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="3.2999878dip" />
            <solid android:color="@color/greyTipsText" />
            <stroke android:width="1.0dip" android:color="@color/greyTipsText" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="3.2999878dip" />
            <solid android:color="@color/greyKeyboardSolid" />
            <stroke android:width="1.0dip" android:color="@color/greyTipsText" />
        </shape>
    </item>
</selector>
